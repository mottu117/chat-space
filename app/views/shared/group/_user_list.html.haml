%div.chat-group-user.clearfix{ id: "chat-group-user-#{user.id}" }
  %input{name: "group[user_ids][]", type: "hidden", value: user.id }
  %p.chat-group-user__name
    = user.nickname
  %a.user-search-remove.chat-group-user__btn.chat-group-user__btn--remove{ data:{ user:{ id: user.id }, user:{ name: user.nickname } } }
    = "削除"
